# Environment Variables Template for Jupyter ML Project
# Copy this file to .env and fill in your actual values
# .env is already in .gitignore for security

# GitHub Configuration
GITHUB_USERNAME=ematavfr
GITHUB_EMAIL=your-email@example.com
GITHUB_TOKEN=ghp_your_personal_access_token_here

# Machine Learning APIs
HUGGINGFACE_TOKEN=hf_your_huggingface_token_here
OPENAI_API_KEY=sk-your_openai_api_key_here
WANDB_API_KEY=your_weights_and_biases_key_here

# Cloud Storage (optional)
AWS_ACCESS_KEY_ID=your_aws_access_key
AWS_SECRET_ACCESS_KEY=your_aws_secret_key
GCP_SERVICE_ACCOUNT_KEY_PATH=/home/jovyan/credentials/gcp-service-account.json

# Database Connections (optional)
DATABASE_URL=postgresql://user:password@host:port/database
REDIS_URL=redis://user:password@host:port

# MLflow Tracking (optional)
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_S3_ENDPOINT_URL=https://s3.amazonaws.com

# Model Registry
MODEL_REGISTRY_URL=your_model_registry_url

# Jupyter Configuration
JUPYTER_CONFIG_DIR=/home/jovyan/config

